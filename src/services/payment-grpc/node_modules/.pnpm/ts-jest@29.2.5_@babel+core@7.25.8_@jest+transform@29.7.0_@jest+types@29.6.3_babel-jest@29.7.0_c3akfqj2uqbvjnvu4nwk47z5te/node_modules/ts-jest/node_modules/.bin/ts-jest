#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.8_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_c3akfqj2uqbvjnvu4nwk47z5te/node_modules/ts-jest/node_modules:/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.8_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_c3akfqj2uqbvjnvu4nwk47z5te/node_modules:/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.8_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_c3akfqj2uqbvjnvu4nwk47z5te/node_modules/ts-jest/node_modules:/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.25.8_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_c3akfqj2uqbvjnvu4nwk47z5te/node_modules:/home/hesam/PhpstormProjects/micro/micro-shop/micro-service/src/services/payment-grpc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
